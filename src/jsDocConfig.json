{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
      "destination": "./dist/docs"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "recurseDepth": 10,
    "sourceType": "module",
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,      
      "outputSourceFiles": true,
      "outputSourcePath": true,
      "path": "ink-docstrap",
      "theme": "spacelab",
      "navType": "vertical",
      "linenums": true,
      "dateFormat": "MMMM Do YYYY, h:mm:ss a"
    }
  }